    "ast-grep-for-swift": {
      "type": "domain",
      "enforcement": "suggest",
      "priority": "high",
      "promptTriggers": {
        "keywords": [
          "find",
          "search for",
          "locate",
          "swift code",
          "method",
          "function",
          "class",
          "property",
          "@mainactor",
          "@observable",
          "@model",
          "force unwrap",
          "refactor",
          "all instances"
        ],
        "intentPatterns": [
          "where.*defined",
          "find.*swift",
          "search.*code",
          "locate.*function",
          "show.*all.*methods",
          "list.*classes"
